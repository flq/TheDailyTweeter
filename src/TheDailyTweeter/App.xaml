<Application x:Class="Membus.WpfTwitterClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
             xmlns:Startup="clr-namespace:Membus.WpfTwitterClient.Startup"
             >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <Startup:AppBootstrap x:Key="bootstrap" />
                </ResourceDictionary>
                <ResourceDictionary Source="Resources/ExpressionDark.xaml" />
                <ResourceDictionary Source="Resources/Local.xaml" />
                <ResourceDictionary Source="Resources/InteractionButtonResources.xaml" />
                <ResourceDictionary Source="Resources/InteractionButtonTemplates.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
